import { setToken, getToken, removeToken } from '../auth.utils';

// ...

const handleSubmit = async (e: React.FormEvent) => {
  // ...
  try {
    const response = await fetch('/api/login', {
      // ...
    });
    const data = await response.json();
    setToken(data.token);
    dispatch(login(data.token));
    console.log(data);
  } catch (error) {
    console.error(error);
  }
};

// ...

const logout = () => {
  removeToken();
  dispatch(logout());
};